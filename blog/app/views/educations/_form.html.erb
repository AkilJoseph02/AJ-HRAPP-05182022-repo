<%= form_for([@resume, @resume.educations.build]) do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :time %><br>
    <%= f.text_field :time %>
  </p>
  <p>
    <%= f.label :degree %><br>
    <%= f.text_area :degree %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>